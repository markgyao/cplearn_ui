# Environment Configuration Template

# Server Configuration
REACT_APP_API_URL=https://test.test/test   # Replace with your API base URL
REACT_APP_API_KEY=your_api_key_here       # Replace with your API key

# App Settings
REACT_APP_APP_NAME=My React App           # Replace with your app name
REACT_APP_DEBUG=true                      # Set to false in production

# Authentication
REACT_APP_AUTH_TOKEN=your_auth_token_here # Replace with your authentication token

# Analytics
REACT_APP_ANALYTICS_ID=your_analytics_id  # Replace with your analytics ID

# Other Variables
REACT_APP_CUSTOM_SETTING=value_here       # Add any custom settings here
